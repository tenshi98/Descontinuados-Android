                    android:id="@+id/label_Provincia"
                    android:textColor="@color/txt_remarcado"
                    android:textStyle="bold"
                    android:layout_marginTop="8dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_no_data"
                    android:id="@+id/Provincia"
                    android:textColor="@color/txt_texto"
                    android:textStyle="bold"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_marginLeft="5dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/hint_Distrito"
                    android:id="@+id/label_Distrito"
                    android:textColor="@color/txt_remarcado"
                    android:textStyle="bold"
                    android:layout_marginTop="8dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_no_data"
                    android:id="@+id/Distrito"
                    android:textColor="@color/txt_texto"
                    android:textStyle="bold"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_marginLeft="5dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
